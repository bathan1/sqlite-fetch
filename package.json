{
    "name": "sqlite-fetch",
    "version": "0.9.8",
    "type": "module",
    "description": "web apis on sqlite",
    "scripts": {
        "cmake:dynamic": "rm -rf build && mkdir build && cd build && cmake .. && make fetch",
        "cmake:static": "rm -rf build && mkdir build && cd build && cmake .. -DTHIRDPARTY_STATIC=ON && make fetch",
        "cmake:ext": "rm -rf build && mkdir build && cd build && cmake .. -DBUILD_FETCH=OFF -DBUILD_EXT=ON && make",
        "test:chrome": "vitest --project web --browser=chromium",
        "test:firefox": "vitest --project web --browser=firefox",
        "test:native": "vitest --project native",
        "sanity-check": "python3 tests/test_sanity_check.py",
        "build": "rm -rf dist && npx concurrently \"npx vite build\" \"npx tsc -p tsconfig.build.json && npx tsc-alias -p tsconfig.build.json\"",
        "postbuild": "pnpm run cmake:ext && cp build/*.so dist/",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "predocs": "typedoc",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs && cp _headers ./docs/.vitepress/dist",
        "docs:preview": "vitepress preview docs",
        "ts-node": "node --loader ts-node/esm"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./wasm.js": {
            "types": "./dist/wasm.d.ts",
            "node": "./dist/wasm.node.js",
            "import": "./dist/wasm.js",
            "default": "./dist/wasm.js"
        }
    },
    "files": [
        "dist",
        "tsconfig.json",
        "bin",
        "README.md"
    ],
    "bin": {
        "sqlite-fetch": "./bin/cli.mjs"
    },
    "keywords": [],
    "author": "bathan1",
    "license": "ISC",
    "peerDependencies": {
        "@sqlite.org/sqlite-wasm": "^3.49.1-build2",
        "kysely": "^0.28.2"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20250605.0",
        "@eslint/js": "^9.30.0",
        "@hono/vite-dev-server": "^0.19.1",
        "@tanstack/react-query": "^5.67.2",
        "@types/better-sqlite3": "^7.6.13",
        "@types/clarinet": "^0.12.3",
        "@types/fhir": "^0.0.41",
        "@types/json-schema": "^7.0.15",
        "@types/node": "^22.16.2",
        "@types/react": "18.2.0",
        "@types/react-dom": "^19.1.6",
        "@vitest/browser": "^3.2.4",
        "@vitest/coverage-istanbul": "3.1.2",
        "@vitest/coverage-v8": "2.1.9",
        "@vitest/ui": "3.2.4",
        "@vitest/web-worker": "^3.2.4",
        "better-sqlite3": "^12.2.0",
        "concurrently": "^9.1.2",
        "eslint": "^9.30.1",
        "fflate": "^0.8.2",
        "globals": "^16.3.0",
        "hono": "^4.7.11",
        "inquirer": "^12.6.3",
        "inquirer-autocomplete-standalone": "^0.8.1",
        "pagefind": "^1.3.0",
        "prettier": "^3.4.2",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "shiki": "^3.6.0",
        "terser": "^5.43.1",
        "ts-morph": "^25.0.0",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.16",
        "tsup": "^8.4.0",
        "tsx": "^4.19.2",
        "typedoc": "^0.28.7",
        "typedoc-plugin-markdown": "^4.7.1",
        "typedoc-plugin-merge-modules": "^7.0.0",
        "typedoc-plugin-rename-defaults": "^0.7.3",
        "typedoc-vitepress-theme": "^1.1.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.35.1",
        "vite": "^7.0.3",
        "vitepress": "^1.6.3",
        "vitest": "^3.2.4",
        "vue": "^3.5.17",
        "wrangler": "^4.4.0",
        "xhr2": "^0.2.1"
    },
    "dependencies": {
        "esm-env": "^1.2.2",
        "eventemitter3": "^5.0.1",
        "execa": "^9.6.0",
        "jsonpointer": "^5.0.1",
        "kysely-codegen": "^0.18.5",
        "kysely-generic-sqlite": "^1.2.1",
        "kysely-wasm": "^1.2.1",
        "openai": "^5.19.1",
        "ora": "^8.2.0",
        "remote-web-streams": "^0.2.0"
    }
}
